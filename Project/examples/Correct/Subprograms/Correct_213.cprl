// procedure with a return statement
// output should be:
// x = 5, y = 6
// x = 8, y = 5

var x, y : Integer;

procedure p(var m : Integer, n : Integer) is
begin
   if m < n then
      return;
   else
      m := n + 3;
   end if;
end p;

begin
   x := 5;
   y := 6;
   p(x, y);
   writeln "x = ", x, ", y = ", y;

   x := 6;
   y := 5;
   p(x, y);
   writeln "x = ", x, ", y = ", y;
end.
