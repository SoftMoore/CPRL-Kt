// Correct output is as follows:
// m = 8, n = 6, k = 2
// x = 8, y = 6

var x, y : Integer;

procedure p(var m : Integer, n : Integer) is

   var k : Integer;

begin
   k := 2;
   m := n + k;
   writeln "m = ", m, ", n = ", n, ", k = ", k;
end p;

begin
   x := 5;
   y := 6;
   p(x, y);
   writeln "x = ", x, ", y = ", y;
end.
